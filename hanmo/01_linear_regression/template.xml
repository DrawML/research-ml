<?xml version="1.0" encoding="utf-8" ?>
<model type="linear_regression">

    <initializer type = "random_uniform">
        <min>-1.0</min>
        <max>1.0</max>
    </initializer>

    <optimizer type="gradient_descent">
        <learning_rate>0.01</learning_rate>
    </optimizer>

    <iteration>1024</iteration>

</model>